@model IEnumerable<BK.UserManagement.Web.Models.ProfileResource>

    <div class="modal fade">
        <div class="modal-dialog" style="width:860px;">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        &times;
                    </button>
                    <h4 class="modal-title">Invoice Number:<b class="text-primary"> @Model.InvoiceNo</b></h4>
                </div>
                <div class="modal-body">
                    @if (!string.IsNullOrEmpty(Model.InvoiceFile))
                    {
                        //data: image / png; base64
                        //<embed type="application/pdf" src="data:application/pdf;base64,@Convert.ToBase64String(Model.InvoiceImage)" width="800" height="800" @*class="img-thumbnail"*@>
                        <embed src="./Document/@Model.InvoiceFile" type="application/pdf" width="800" height="800" />
                    }
                </div>
            </div>
        </div>
    </div>
