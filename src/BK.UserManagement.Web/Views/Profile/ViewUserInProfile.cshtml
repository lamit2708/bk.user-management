@model IEnumerable<BK.UserManagement.Web.Models.UserModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        User in profile: @ViewBag.ProfileName
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Profile</a></li>
        <li class="active"><a href="#">Index</a></li>

    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <!-- /.box-header -->
                <div class="box-header">
                    
                </div>

                <div class="box-body">
                    <table id="tbl_userIn" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>Username</th>
                                <th><span class="pull-right">Features</span></th>
                            </tr>
                        </thead>
                        <tbody>



                            @foreach (var u in Model)
                            {
                                <tr>
                                    <td>
                                        @u.USERNAME
                                    </td>

                                    <td>

                                    </td>
                                </tr>
                            }
                        </tbody>

                        <tfoot>
                            <tr>
                                @*<th>Profile Name</th>
                                    <th>Features</th>*@
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->

    <div>
        <i class="fa fa-backward"></i><a asp-action="Index"> Back to List</a>
    </div>

</section>
<!-- /.content -->




@section Scripts {

    <!-- page script -->
    <script type="text/javascript">
        $(function () {
            //$('#example1').DataTable()
            $('#tbl_userIn').DataTable({
                'paging': true,
                'lengthChange': true,
                'searching': true,
                'ordering': true,
                'info': true,
                'pageLength': 25,
                'autoWidth': false       
            })
        })
    </script>
}